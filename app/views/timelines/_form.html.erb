<%= form_for @timeline do |f| %>
  <div class="review-form-list">
    <%=f.label :title, "Title"%>
    <%= f.text_field :title %>
  </div>

  <div class="review-form-list">
    <%=f.label :description, "Description"%>
    <%= f.text_field :description %>
  </div>

  <ul class="review-form-list">
  <%= f.label :personal, "Private" %>
  <%= f.select(:personal, options_for_select([true, false])) %>
  <p> (Private by default)</p>
</ul>


  <div class="review-form-list no-bullet">
    <%= f.submit class: "button tiny"%>
  </div>
<% end %>
